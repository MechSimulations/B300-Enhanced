<ModelBehaviors>
	<Template Name="ASOBO_RMP_BACKLIGHT_Template">
		<DefaultTemplateParameters>
			<BACKLIGHT_NAME>PUSH_RADIO#SIDE#_#BASE_NAME#_SEQ1</BACKLIGHT_NAME>
		</DefaultTemplateParameters>
		
		<Component ID="#BACKLIGHT_NAME#" Node="#BACKLIGHT_NAME#">
			<UseTemplate Name="ASOBO_GT_Emissive_Gauge">
				<Condition Check="POTENTIOMETER">
					<True>
						<EMISSIVE_CODE>(L:XMLVAR_RMP_#SIDE#_On, bool) (A:LIGHT POTENTIOMETER:#POTENTIOMETER#, Percent over 100) *</EMISSIVE_CODE>
					</True>
					<False>
				<EMISSIVE_CODE>(L:XMLVAR_RMP_#SIDE#_On, bool)</EMISSIVE_CODE>
					</False>
				</Condition>
			</UseTemplate>
		</Component>
	</Template>

	<Template Name="ASOBO_RMP_BUTTON_Template">
		<DefaultTemplateParameters>
			<NODE_ID>PUSH_RADIO#SIDE#_#BASE_NAME#</NODE_ID>
			<ANIM_NAME_BUTTON>PUSH_RADIO#SIDE#_#BASE_NAME#</ANIM_NAME_BUTTON>
			<LED_NODE_ID>PUSH_RADIO#SIDE#_#BASE_NAME#_SEQ2</LED_NODE_ID>
			<EVENT_NAME>A320_Neo_FDW_BTN_#SIDE#_#BASE_NAME#</EVENT_NAME>
			<STATE_VAR_NAME>XMLVAR_RMP_#SIDE#_#BASE_NAME#_Active</STATE_VAR_NAME>
			<OPTIONAL_ACTIVE_IF>1</OPTIONAL_ACTIVE_IF>
			<RUN_ON_PRESS></RUN_ON_PRESS>
			<WWISE_EVENT_1>radio_push_button_on</WWISE_EVENT_1>
			<NORMALIZED_TIME_1>0.1</NORMALIZED_TIME_1>
			<WWISE_EVENT_2>radio_push_button_off</WWISE_EVENT_2>
			<NORMALIZED_TIME_2>0.5</NORMALIZED_TIME_2>
			<TOOLTIPID>TT:COCKPIT.TOOLTIPS.RADIO_SELECT_#BASE_NAME#</TOOLTIPID>
		</DefaultTemplateParameters>
		<EditableTemplateParameters>
			<AddParams>
				<LED_NODE_ID></LED_NODE_ID>
			</AddParams>
		</EditableTemplateParameters>
		
		<Component ID="#NODE_ID#" Node="#NODE_ID#">
			<UseTemplate Name="ASOBO_GT_Push_Button">
				<ANIM_NAME>#ANIM_NAME_BUTTON#</ANIM_NAME>
				<LEFT_SINGLE_CODE>
					<!--Only respond when power is on-->
					(L:XMLVAR_RMP_#SIDE#_On) #OPTIONAL_ACTIVE_IF# and if{
						(&gt;H:#EVENT_NAME#) (L:#STATE_VAR_NAME#) ! (&gt;L:#STATE_VAR_NAME#)
						#RUN_ON_PRESS#
					}
				</LEFT_SINGLE_CODE>
			</UseTemplate>
		</Component>
		
		<Component ID="#LED_NODE_ID#" Node="#LED_NODE_ID#">
			<UseTemplate Name="ASOBO_GT_Emissive_Gauge">
				<EMISSIVE_CODE>
					(L:XMLVAR_RMP_#SIDE#_On, bool) if{
						(L:#STATE_VAR_NAME#, bool) #OPTIONAL_ACTIVE_IF# and
					}
					els{ 0 }
				</EMISSIVE_CODE>
			</UseTemplate>
		</Component>
	</Template>

	<Template Name="ASOBO_RMP_BUTTON_WITH_LIGHTS_Template">
		<DefaultTemplateParameters>
			<BASE_NAME>UNKNOWN</BASE_NAME>
		</DefaultTemplateParameters>
		
		<UseTemplate Name="ASOBO_RMP_BACKLIGHT_Template">
		</UseTemplate>
		
		<UseTemplate Name="ASOBO_RMP_BUTTON_Template">
		</UseTemplate>
	</Template>

	<Template Name="ASOBO_RMP_GROUPED_BUTTON_Template">
		<DefaultTemplateParameters>
			<NODE_ID>PUSH_RADIO#SIDE#_#BASE_NAME#</NODE_ID>
			<ANIM_NAME_BUTTON>PUSH_RADIO#SIDE#_#BASE_NAME#</ANIM_NAME_BUTTON>
			<LED_NODE_ID>PUSH_RADIO#SIDE#_#BASE_NAME#_SEQ2</LED_NODE_ID>
			<EVENT_NAME>A320_Neo_FDW_BTN_#SIDE#_#BASE_NAME#</EVENT_NAME>
			<GROUP_STATE_VAR_NAME>XMLVAR_RMP_#SIDE#_GROUP_#GROUP_NAME#_IS_DISABLED</GROUP_STATE_VAR_NAME>
			<CURRENT_BUTTON_VAR_NAME>XMLVAR_RMP_#SIDE#_CURRENT_BUTTON_IN_GROUP_#GROUP_NAME#</CURRENT_BUTTON_VAR_NAME>
			<OPTIONAL_ACTIVE_IF>1</OPTIONAL_ACTIVE_IF>
			<RUN_ON_PRESS></RUN_ON_PRESS>
			<WWISE_EVENT_1>radio_push_button_on</WWISE_EVENT_1>
			<NORMALIZED_TIME_1>0.1</NORMALIZED_TIME_1>
			<WWISE_EVENT_2>radio_push_button_off</WWISE_EVENT_2>
			<NORMALIZED_TIME_2>0.5</NORMALIZED_TIME_2>
			<TOOLTIPID>TT:COCKPIT.TOOLTIPS.RADIO_SELECT_#BASE_NAME#</TOOLTIPID>
		</DefaultTemplateParameters>
		
		<Component ID="#NODE_ID#" Node="#NODE_ID#">
			<UseTemplate Name="ASOBO_GT_Push_Button">
				<ANIM_NAME>#ANIM_NAME_BUTTON#</ANIM_NAME>
				<LEFT_SINGLE_CODE>
					<!--Only respond when power is on AND the group isn't disabled AND the optional requirement is valid-->
					(L:XMLVAR_RMP_#SIDE#_On) if{
						(L:#GROUP_STATE_VAR_NAME#) ! if{
							#OPTIONAL_ACTIVE_IF# if{
								(&gt;H:#EVENT_NAME#) #GROUP_INDEX# (&gt;L:#CURRENT_BUTTON_VAR_NAME#)
								#RUN_ON_PRESS#
							}
						}
					}
				</LEFT_SINGLE_CODE>
			</UseTemplate>
		</Component>
		
		<Component ID="#LED_NODE_ID#" Node="#LED_NODE_ID#">
			<UseTemplate Name="ASOBO_GT_Emissive_Gauge">
				<EMISSIVE_CODE>
					(L:XMLVAR_RMP_#SIDE#_On, bool) (L:#GROUP_STATE_VAR_NAME#) ! and if{
						(L:#CURRENT_BUTTON_VAR_NAME#) #GROUP_INDEX# == #OPTIONAL_ACTIVE_IF# and
					}
					els{
						0
					}
				</EMISSIVE_CODE>
			</UseTemplate>
		</Component>
	</Template>

